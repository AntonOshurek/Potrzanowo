---
import { getEntry } from 'astro:content';
//STYLES
import './main.scss';
//COMPONENTS
import IndexLink from '../index-link/index-link.astro';

//CONTENT
const investmentlink = await getEntry('indexPageLinks', 'index-link-to-investment');
const locationlink = await getEntry('indexPageLinks', 'index-link-to-location');
const activitylink = await getEntry('indexPageLinks', 'index-link-to-activity');
const contactlink = await getEntry('indexPageLinks', 'index-link-to-contact');
---

<main class="main">
	<IndexLink data={investmentlink} />
	<IndexLink data={locationlink} />
	<IndexLink data={activitylink} />
	<IndexLink data={contactlink} />
</main>
